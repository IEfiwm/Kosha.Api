          <value>_x0040_projectRef,ProjectRef,0,10</value>
        </Parameters>
        <SqlCommand>SELECT Kosha_UserProjects.PersonnelCode, Kosha_UserProjects.FirstName,
  Kosha_UserProjects.LastName, Kosha_ProjectBankAccount.BranchName,
  Kosha_ProjectBankAccount.Title, Kosha_ProjectBankAccount.BranchCode,
  Kosha_ProjectBankAccount.AccountNumber, Kosha_UserProjects.NationalCode,
  Kosha_UserProjects.AccountNumber as UserAccountNumber, Kosha_UserProjects.Salary,
  Kosha_UserProjects.ماه, Kosha_UserProjects.[سال ]
FROM Kosha_UserProjects INNER JOIN
  Kosha_ProjectBankAccount ON Kosha_UserProjects.BankId =
    Kosha_ProjectBankAccount.BankId
WHERE projectRef =@projectRef AND [ماه]=@Month AND [سال ]=@Year</SqlCommand>
      </DataSource>
