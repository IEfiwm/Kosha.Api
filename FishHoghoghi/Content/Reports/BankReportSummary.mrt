        </Parameters>
        <SqlCommand>SELECT Kosha_ProjectBankAccount.BankId,Sum(Convert(decimal,Salary)) as Salary,
	Title, Kosha_ProjectBankAccount.AccountNumber,Count(*) as count
	FROM Kosha_UserProjects INNER JOIN
	Kosha_ProjectBankAccount ON Kosha_UserProjects.BankId =
    Kosha_ProjectBankAccount.BankId
WHERE projectRef =@projectRef AND [ماه]=@Month AND [سال ]=@Year
Group by Kosha_ProjectBankAccount.BankId,Title, Kosha_ProjectBankAccount.AccountNumber</SqlCommand>
